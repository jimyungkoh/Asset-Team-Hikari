# ============================================================
# Server Environment Variables Example
# ============================================================

# Server 기본 설정
NEST_PORT=3001
PORT=3001  # NEST_PORT 우선순위가 높음
NEST_CORS_ORIGINS=  # CORS 허용 origin (쉼표로 구분, 예: http://localhost:3000,https://example.com)

# 내부 인증 설정
INTERNAL_API_TOKEN=  # 내부 API 인증 토큰
SKIP_TOKEN_AUTH=false  # 개발 환경에서 토큰 인증 스킵 (true/false)

# Python 서비스 설정
PYTHON_SERVICE_URL=http://localhost:8000  # TradingAgents Python 서비스 URL

# AWS 기본 설정
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=  # 로컬 개발 시에만 필요 (프로덕션은 IAM 역할 사용)
AWS_SECRET_ACCESS_KEY=  # 로컬 개발 시에만 필요

# DynamoDB 설정
DYNAMODB_TABLE_NAME=TickerDailyArtifacts

# OpenRouter 설정 (종합 리포트 생성에 사용)
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
REPORT_SUMMARY_DEFAULT_LANG=ko
OPENROUTER_SUMMARY_MODEL=anthropic/claude-haiku-4.5
OPENROUTER_HTTP_REFERER=https://your-domain.example.com
OPENROUTER_X_TITLE=TradingAgents Composite Reports

# 데이터베이스 연결 (직접 PostgreSQL 접속 문자열)
# 예: postgresql://postgres:password@db.<project>.supabase.co:5432/postgres
DB_URL=
