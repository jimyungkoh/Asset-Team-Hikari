# DO NOT commit real API keys. This file is for documentation only.
# Copy to .env and fill with your credentials locally.

# Core APIs
OPENAI_API_KEY=your-openai-api-key
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-api-key

# OpenRouter (used when provider is set to OpenRouter in CLI)
OPENROUTER_API_KEY=your-openrouter-api-key
OPENROUTER_SITE_URL=https://your.site
OPENROUTER_APP_TITLE=TradingAgents

# Gemini (used for news browsing under OpenRouter mode)
GOOGLE_API_KEY=your-google-api-key
GEMINI_WEB_MODEL=gemini-2.5-flash  # Default model for Gemini

# Optional configs
TRADINGAGENTS_RESULTS_DIR=./results  # Output directory for results (default: ./results)
TRADINGAGENTS_LLM_PROVIDER=openai  # LLM provider: openai, openrouter
TRADINGAGENTS_DEEP_THINK_LLM=o4-mini  # Model for deep thinking tasks
TRADINGAGENTS_QUICK_THINK_LLM=gpt-4o-mini  # Model for quick tasks
TRADINGAGENTS_BACKEND_URL=https://api.openai.com/v1  # Backend URL for API
TRADINGAGENTS_THINKING_MODE=false  # Enable thinking mode (true/false)
TRADINGAGENTS_THINKING_EFFORT=medium  # Thinking effort level: low, medium, high
TRADINGAGENTS_THINKING_EFFORT_DEEP=medium  # Effort for deep thinking
TRADINGAGENTS_THINKING_EFFORT_QUICK=medium  # Effort for quick thinking

# Optional local providers
# OLLAMA_HOST=http://localhost:11434
